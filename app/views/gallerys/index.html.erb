<h2 class="mt-4 d-flex justify-content-center">＜写真一覧＞</h2>

<div class="row">
  <div class="col-md-9">
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>写真</th>
        </tr>
      </thead>
      <tbody>
        <% @events.each do |event| %>
        <tr>
          <td><%= event.image %></td>
        </tr>
        <% end %>
      </tbody>

    </table>
  </div>
</div>